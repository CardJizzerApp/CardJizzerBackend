    "express-ws": "^4.0.0",
    "google-auth-library": "^5.2.1",
    "mongo": "^0.1.0",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.0",
    "querystring": "^0.2.0",
    "redis": "^2.8.0",
    "uuid": "^3.3.2",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "del": "^5.0.0",
    "eslint": "^6.2.1",
